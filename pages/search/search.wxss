/* pages/search/search.wxss */
.v1 {
  width:100%;
  height: 90rpx;
  background-color: red;
}

.v1 view:nth-of-type(2){
  width: 600rpx;
  height: 60rpx;
  position: absolute;
  margin: 15rpx 70rpx;
  background-color: white;
}

.v1 view:nth-of-type(1){
  width: 60rpx;
  height:60rpx;
  border-radius: 50%;
  position: absolute;
  margin: 15rpx 40rpx;
  background-color: white;
}

.v1 view:nth-of-type(3){
  width: 60rpx;
  height:60rpx;
  border-radius: 50%;
  position: absolute;
  margin: 15rpx 640rpx;
  background-color: white;
}
.v1 input{
  width: 600rpx;
  height: 60rpx;
}

block1{
  width:100%;
  height: 100%;
}

.v3{
  width:100%;
  height: 100rpx;
  position:relative;
  box-shadow: 1px 2px 3px #d9d9d9;
}

.v3 view{
  height: 100rpx;
  width: 380rpx;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  flex-direction: column;
  margin:10rpx 200rpx;
  overflow:hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
}

.v3 navigator{
  width:100%;
  height: 100%; 
}

.v3 image{
  width:90rpx;
  height:90rpx;
  margin:5rpx 70rpx;
  position:absolute;
}

.v3 text:nth-of-type(2){
 width: 300rpx;
 font-size:75%;
 overflow:hidden;
 white-space:nowrap;
 text-overflow: ellipsis;
}

.v3 text:nth-of-type(1){
 font-size:95%;
 width: 390rpx;
 overflow:hidden;
 white-space:nowrap;
 text-overflow: ellipsis;
}

.v3 text2{
  display: flex;
  justify-content: center;
  align-items: flex-start;
  flex-direction: column;
  margin:-80rpx 620rpx;
}

.v3 text2{
 font-size:80%;
}

.view2{
  height:100rpx;
}

block2 text{
  height: 100rpx;
  weight:100rpx;
  margin: auto auto;
}